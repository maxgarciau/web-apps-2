<%# TODO: company details page %>
<h1><%= @company["name"]%></h1>
<h2><%= @company["city"]%>, <%= @company["state"]%></h2>

<ul>
  <% for contact in @contacts %>
    <li>
      <%= contact["first_name"] %> <%= contact["last_name"] %>
    </li>
  <% end %>
</ul>
<p>
  <a href="/contacts/new?company_id=<%= @company["id"] %>">Create a new contact for <%= @company["name"]%></a>
</p>

<p>
  <a href="/companies">Return</a>
</p>
